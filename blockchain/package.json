{
  "name": "nft-rent-marketplace",
  "version": "1.0.0",
  "description": "The NFT Rent Marketplace is a unique project that leverages the power of Ethereum's ERC721 standard to create a marketplace where users can rent Non-Fungible Tokens (NFTs). This project showcases how NFTs can be utilized beyond buying and selling, opening up a new realm of possibilities for NFT use-cases.",
  "scripts": {
    "start:dev": "nodemon api/server.js",
    "start": "node api/server.js",
    "compile": "hardhat compile",
    "build": "npx thirdweb@latest detect",
    "deploy": "npx thirdweb@latest deploy",
    "publish": "npx thirdweb@latest publish",
    "lint": "npm run lint:contracts && npm run format:check",
    "lint:fix": "solhint 'contracts/**/*.sol' --fix",
    "lint:contracts": "solhint 'contracts/*.sol'",
    "lint:contracts:fix": "solhint 'contracts/**/*.sol' --fix",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write ."
  },
  "author": "trexx",
  "devDependencies": {
    "@chainlink/contracts": "0.5.1",
    "@ethersproject/abi": "^5.7.0",
    "@ethersproject/providers": "^5.7.1",
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.3",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.6",
    "@nomicfoundation/hardhat-toolbox": "^2.0.0",
    "@nomiclabs/hardhat-ethers": "^2.2.2",
    "@nomiclabs/hardhat-etherscan": "^3.1.0",
    "@openzeppelin/hardhat-upgrades": "^1.22.1",
    "@typechain/ethers-v5": "^10.1.0",
    "@typechain/hardhat": "^6.1.3",
    "chai": "^4.3.6",
    "eth-crypto": "^2.4.0",
    "ethers": "^5.7.2",
    "hardhat": "^2.14.0",
    "hardhat-contract-sizer": "^2.6.1",
    "hardhat-gas-reporter": "^1.0.9",
    "prettier": "^2.7.1",
    "prettier-plugin-solidity": "^1.0.0-beta.24",
    "solhint": "^3.3.7",
    "solhint-plugin-prettier": "^0.0.5",
    "solidity-coverage": "^0.8.2",
    "typechain": "^8.1.0"
  },
  "dependencies": {
    "@chainlink/contracts": "^0.6.1",
    "@chainlink/env-enc": "^1.0.5",
    "@openzeppelin/contracts": "4.9.0",
    "@openzeppelin/contracts-upgradeable": "4.9.0",
    "@thirdweb-dev/chains": "^0.1.19",
    "@thirdweb-dev/contracts": "^3.5.4",
    "@thirdweb-dev/sdk": "^3.10.19",
    "axios": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "nodemon": "^2.0.22"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": false,
    "printWidth": 120
  }
}
